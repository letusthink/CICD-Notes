kind: pipeline   
type: kubernetes  
name: default    

clone:
  depth: 1

steps:
- name: step1  
  pull: if-not-exsits
  image: alpine:3.16
  environment:
    group_secert:
      from_secret: org_token  
    ck_secert:
      from_secret: my_token  
    test:
      from_secret: test            
  commands:
  - echo "$group_secert"
  - echo "$ck_secert"
  - echo "$test"

